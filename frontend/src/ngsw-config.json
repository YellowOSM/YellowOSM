{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "osm",
      "urls": [
        "//maps.wikimedia.org/**"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 1024,
        "maxAge": "24h",
        "timeout": "2s",
        "strategy": "performance"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*__*",
    "!/**/*__*/**"
  ]
}
